@model List<WhatIHaveModel>
    <div class="col col-md-4">
        <div class="list-group">
            @foreach (var m in Model)
            {
                <a href="#" class="list-group-item list-group-item-action flex-column align-items-start">
                    <div class="d-flex w-100 justify-content-between">
                        <h5 class="mb-1">@m.ProductName</h5>
                        <small>@m.Amount</small>
                    </div>
                </a>
            }
        </div>
        <a asp-controller="AddInventory" asp-action="Index" class="btn btn-primary shadow-sm mt-2"><i class="fas fa-user fa-sm text-white-50"></i> Add to my Inventory</a>

    </div>
